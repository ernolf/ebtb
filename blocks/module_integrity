[ -f "${F[lmj]}" ] || ${V[curl]} -o "${F[lmj]}" -A "$(fp)$A" ${V[cargs]}/${F[rmj]}
q2 ${X[jq]} -r '.modules[] | "\(.hash) \(.name)"' ${F[lmj]} >${D[li]}/modules.sha256
cd ${D[li]}
tntd=($(
    q2 ${X[sha256sum]} -c --ignore-missing --quiet modules.sha256 | ${X[sed]} 's/:.*//'
))
(( ${#tntd[@]} )) && {
    for x in ${tntd[@]}; do
        q0 ${X[rm]} "$x" || B[r]=_
    done
    ${B[r]:-:} && $CMA
}
cd
