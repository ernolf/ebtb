[ -f "${F[lmj]}" ] || c -o "${F[lmj]}" -A "$(fp)$A" ${V[cargs]}/${F[rmj]}
q2 ${X[jq]} -r '.modules[] | "\(.hash) \(.name)"' ${F[lmj]} >${D[li]}/modules.sha256
q0 chrf ${D[li]} 0
cd ${D[li]}
tntd=($(
    q2 ${X[sha256sum]} -c --ignore-missing --quiet modules.sha256 | sed_ 's/:.*//'
))
(( ${#tntd[@]} )) && {
    for x in ${tntd[@]}; do
        ${C[r]} "$x" || B[r]=_
    done
    ${B[r]:-:} && exec $CMA
}
cd
q0 chrf ${D[li]} 0
