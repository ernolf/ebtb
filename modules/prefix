${PREFIX_BASHLIB:-false} || {
    PREFIX_BASHLIB=:

    . <(lrequire ansi)

    # Prefix
    pf(){
        printf ' - '
    }

    # Exclamation
    xpf(){
        R;printf "⚠️$(N):"
    }

    # Information
    ipf(){
        printf "I:"
    }

    # Question
    qpf(){
        printf 'Q: '
    }
}
