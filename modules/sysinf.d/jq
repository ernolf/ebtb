${SYSINF_JQ_BASHLIB:-_} && return
SYSINF_JQ_BASHLIB=:

sysinf_jq(){
    local -A bool
    while (( $# )); do
        [[ $1 ]] && bool[$1]=:
        shift
    done
    ${bool[ff]:-_} || {
        ${bool[spinner]} && {
            kill_spinner
            spinner Yi " . determine system informations - jq version ." &
        }
        SYSINF+=("echo jq version: $(${X[jq]} --version)")
    }
}
