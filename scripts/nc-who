## SCRIPT SPECIFIC

contribute_(){
    . <(require contribute)
    contribute "https://help.nextcloud.com/t/script-nc-who-similar-to-unix-who-follow-live-who-is-logged-in/164972"
}; donate_(){ contribute_;}
[[ "$@" =~ (donat|contrib) ]] && contribute_

declare arg command
declare -a args

for arg in "$@"; do
    case "$arg" in
        --trace) B[trace]=:  ;;
              *) args+=("$arg")
    esac
done
set -- "${args[@]}" && unset -v arg args

command=print_help
while (($#));do
    case "$1" in
        monitor|last|god)
            command="nc_$1"
            break 2
            ;;
        -h|--help)
            print_help
            ;;
        *)  f;echo "--> unknown option: \"$1\" <--"
            print_help
    esac
done

$command

exit0

